@model IEnumerable<FirstApp.Models.Employee>
@{
    ViewBag.Title = "Our employees";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Enterprise</h1>
<h2>Employees</h2>
<table>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.FirstName</td>
            <td>
                <a asp-action="Details" asp-controller="Home" asp-route-id="@employee.Id">Details</a>|
                <a asp-action="Edit" asp-controller="Home" asp-route-id="@employee.Id">Change</a>|
                <a asp-action="Delete" asp-controller="Home" asp-route-id="@employee.Id">Remove</a>
            </td>
        </tr>

    }
</table>
<a asp-action="Create">Add employee</a>
<table class="table">
    <tr><td>First name</td>><td>Last name</td><td>Years stuff</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.FirstName</td><td>@item.LastName</td><td>@item.YearsStuff</td></tr>
    }
</table>

